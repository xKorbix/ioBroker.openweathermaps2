    "@types/gulp": "^4.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.10",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
